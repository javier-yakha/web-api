<Application x:Class="wpf_ui.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:wpf_ui"
             StartupUri="Windows/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Theme/NavbarButton.xaml" />
                <ResourceDictionary Source="Theme/CustomSearchBar.xaml" />
                <ResourceDictionary Source="Theme/ErrorLabel.xaml" />
                <ResourceDictionary Source="Theme/TableTextBlock.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
